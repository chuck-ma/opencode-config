{
  "$schema": "https://opencode.ai/config.json",
  "model": "anthropic/claude-haiku-4-5",
  "permission": {
    "skill": "allow"
  },
  "plugin": [
    "oh-my-opencode@3.1.8",
    "opencode-openai-codex-auth",
    "opencode-antigravity-auth@beta",
    "memory-bank-skill@5.18.0",
    "opencode-google-search@0.1.4"
