  "provider": {
    "cliproxy": {
      "npm": "@ai-sdk/anthropic",
      "options": {
        "baseURL": "http://127.0.0.1:8317/v1",
        "apiKey": "{env:CLIPROXY_API_KEY}"
      },
      "models": {
        "claude-sonnet-4-5-20250929": {
          "limit": {
            "context": 200000,
            "output": 64000
          },
          "modalities": {
            "input": [
              "text",
              "image"
            ],
            "output": [
              "text"
            ]
          },
          "options": {
            "thinking": {
              "type": "enabled",
              "budgetTokens": 16000
            }
          },
          "variants": {
            "low": {
              "options": {
                "thinking": {
                  "type": "enabled",
                  "budgetTokens": 8192
                }
              }
            },
            "high": {
              "options": {
                "thinking": {
                  "type": "enabled",
                  "budgetTokens": 32768
                }
              }
            }
          }
        },
