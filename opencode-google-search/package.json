{
  "name": "opencode-google-search",
  "version": "0.1.4",
  "description": "OpenCode plugin for Google Search via Antigravity Manager",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist/",
    "index.ts",
    "src/",
    "README.md"
