  "repository": {
    "type": "git",
    "url": "git+https://github.com/chuck-ma/opencode-config.git"
  },
  "peerDependencies": {
    "@opencode-ai/plugin": "^1.1.36"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
